#include <stdio.h>
#include <iostream>
#include <algorithm>
#include <math.h>
#include <queue>
#include <stdlib.h>
#include <string>
#include <stack>
using namespace std;

int main()
{
	ios::sync_with_stdio(false);
	cin.tie(0); cout.tie(0);
	int A[5];
	for (int i = 0; i < 5; i++)
	{
		cin >> A[i];
		if (A[i] < 40)
			A[i] = 40;
	}
	printf("%d", (A[0] + A[1] + A[2] + A[3] + A[4]) / 5);
	return 0;
}
