#include <stdio.h>
#include <iostream>
#include <algorithm>
#include <math.h>
#include <queue>
#include <stdlib.h>
#include <string>
#include <stack>
using namespace std;

int main()
{
	ios::sync_with_stdio(false);
	cin.tie(0); cout.tie(0);
	int A[3];
	cin >> A[0] >> A[1] >> A[2];
	sort(A, A + 3);
	printf("%d", A[1]);
	return 0;
}
