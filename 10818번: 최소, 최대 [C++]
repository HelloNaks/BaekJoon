#include <string>
#include <iostream>
#include <vector>
#include <stdio.h>
#include <algorithm>
#include <math.h>
#include <queue>

using namespace std;

int main()
{
	int n, min = 1000000, max = -1000000, tmp;
	cin >> n;
	for (int i = 0; i < n; i++)
	{
		cin >> tmp;
		if (min > tmp)
			min = tmp;
		if (max < tmp)
			max = tmp;
	}
	cout << min << " " << max << endl;
	return 0;
}
