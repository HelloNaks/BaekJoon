#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <iostream>
#include <algorithm>
#include <math.h>
#include <queue>
#include <string.h>
#include <stdlib.h>

using namespace std;

int main()
{
	int n;
	cin >> n;
	int sum = 0;
	char T='0';
	for (int i = 0; i < n; i++)
	{
		cin >> T;
		sum += T - '0';
	}
	cout << sum;

	return 0;
}
