#include <string>
#include <iostream>
#include <vector>
#include <stdio.h>
#include <algorithm>
#include <math.h>
#include <queue>
using namespace std;

int main()
{
	int n;
	scanf("%d", &n);
	if (n % 4 == 0 && n % 100 != 0)
		printf("1");
	else if (n % 4 == 0 && n % 400 == 0)
		printf("1");
	else
		printf("0");
	return 0;
}
