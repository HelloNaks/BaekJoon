#include <stdio.h>

int main()
{
	int n, d3=0;
	scanf("%d", &n);
	if (n % 5 == 0)
		printf("%d", n / 5);
	else
	{
		for (int i = 0;; i++)
		{
			if (n == 0)
				return 0;

			else if (n < 3)
			{
				printf("-1");
				return 0;
			}
			d3++;
			n -= 3;
			if (n % 5 == 0)
			{
				printf("%d", n / 5 + d3);
				return 0;
			}
			else
				continue;
		}
	}
	return 0;
}
