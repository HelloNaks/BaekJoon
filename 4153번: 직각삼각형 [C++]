//#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <iostream>
#include <algorithm>
#include <math.h>
#include <queue>
#include <string>
#include <stdlib.h>
#define max(a,b) a > b ? a : b
using namespace std;

int main()
{
	while (true)
	{
		int a, b, c;
		cin >> a >> b >> c;
		if (a == 0 && b == 0 && c == 0)
			break;
		if (a * a + b * b == c * c || a * a + c * c == b * b || c * c + b * b == a * a)
			printf("right\n");
		else
			printf("wrong\n");
	}
	return 0;
}
