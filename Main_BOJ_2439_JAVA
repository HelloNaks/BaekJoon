import java.io.IOException;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) throws IOException {
		Scanner scann = new Scanner(System.in);
		int A=scann.nextInt();
		for(int i=0;i<A;i++) {
			for (int j = A-1; j>i; j--) {
				System.out.print(" ");
			}
			for (int j = A-i; j <= A; j++) {
				System.out.print("*");
			}
			System.out.println();
		}
	}

}
