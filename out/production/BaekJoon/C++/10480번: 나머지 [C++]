#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <iostream>
#include <algorithm>
#include <math.h>
#include <queue>
#include <string>
#include <stdlib.h>
using namespace std;

int main()
{
	int a, b, c;
	cin >> a >> b >> c;
	printf("%d\n", (a+b)%c);
	printf("%d\n", ((a%c)+(b%c))%c);
	printf("%d\n", (a*b)%c);
	printf("%d\n", ((a%c)*(b%c))%c);
	return 0;
}
