#include <string>
#include <iostream>
#include <vector>
#include <stdio.h>
#include <algorithm>
#include <math.h>
#include <queue>
using namespace std;

int main()
{
	int a;
	scanf("%d", &a);
	if (a >= 90)
		printf("A\n");
	else if (a >= 80)
		printf("B\n");
	else if (a >= 70)
		printf("C\n");
	else if (a >= 60)
		printf("D\n");
	else
		printf("F\n");
	return 0;
}
