import java.util.Arrays;
import java.util.Scanner;

public class Main {

	static int []a=new int[3];
	public static void main(String[] args) {
		Scanner scann = new Scanner(System.in);
		a[0]=scann.nextInt();
		a[1]=scann.nextInt();
		a[2]=scann.nextInt();
		Arrays.sort(a);
		for (int i = 0; i < 3; i++) {
			System.out.print(a[i]+" ");
		}
	}

}
