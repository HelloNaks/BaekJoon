import java.util.Scanner;

public class Main {

	public static int factorial (int a) {
		if(a<2)
            return 1;
        else
            return a * factorial(a-1);
	}
	
	public static void main(String[] args) {
		Scanner scann = new Scanner(System.in);
        int N = scann.nextInt();
        int answer = factorial(N);
        System.out.println(answer);
	}
}
