#include <stdio.h>
#include <iostream>
#include <algorithm>
#include <math.h>
#include <queue>
#include <stdlib.h>
#include <string>
using namespace std;

int main()
{
	int n, tmp, num = 0;
	cin >> n;
	int A[101] = { 0 };

	for (int i = 1; i <= n; i++)
	{
		cin >> tmp;
		if (A[tmp] == 0)
		{
			A[tmp] = 1;
			continue;
		}
		else
		{
			num++;
		}
	}
	cout << num;
	return 0;
}
