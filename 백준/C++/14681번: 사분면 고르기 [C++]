//#include <string>
#include <iostream>
//#include <vector>
//#include <stdio.h>
#include <algorithm>
//#include <math.h>
#include <queue>
//#include <stack>

using namespace std;

int main()
{
	cin.tie(NULL);
	ios::sync_with_stdio(false);
	int a, b;
	cin >> a >> b;
	if (a > 0 && b > 0)
		cout << '1';
	else if (a < 0 && b > 0)
		cout << '2';
	else if (a < 0 && b < 0)
		cout << '3';
	else
		cout << '4';
	return 0;
}
