#include <string>
#include <iostream>
#include <vector>
#include <stdio.h>
#include <algorithm>
#include <math.h>
#include <queue>
using namespace std;

#define min(a,b) a > b ? b : a
#define max(a,b) a > b ? a : b

int main()
{
//빠른 입력과 출력을 위하여 사용하는 
	std::cin.tie(NULL);
	ios::sync_with_stdio(false);
	int t;
	cin >> t;
	for (int i = 0; i < t; i++)
	{
		int a, b;
		cin >> a >> b;
		cout << a + b << '\n';
	}
	return 0;
}
