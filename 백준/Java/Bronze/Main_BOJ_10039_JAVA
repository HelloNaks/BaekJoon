import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner scann = new Scanner(System.in);
		int[] A=new int[5];
		for (int i = 0; i < 5; i++) {
			A[i]=scann.nextInt();
			if(A[i]<40)
				A[i]=40;
		}
		System.out.println((A[0]+A[1]+A[2]+A[3]+A[4])/5);
	}

}
