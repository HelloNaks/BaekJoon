import java.util.Arrays;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner scann = new Scanner(System.in);
		int N=scann.nextInt();
		int []arr=new int[N];
		for(int i=0;i<N;i++) {
			arr[i]=scann.nextInt();
		}
		Arrays.sort(arr);
		System.out.println(arr[0]+" "+arr[N-1]);
	}

}
